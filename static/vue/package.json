    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.3.1",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "typescript": "~4.7.4",
